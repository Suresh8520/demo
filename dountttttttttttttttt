# Set the URL of the zip file you want to download
$sourceZipUrl = "https://example.com/path/to/folder.zip"

# Set the destination path where you want to save the zip file
$destinationZipPath = "C:\Users\YourUsername\Downloads\folder.zip"

# Download the zip file
Invoke-WebRequest -Uri $sourceZipUrl -OutFile $destinationZipPath

# Extract the contents of the zip file to a specific location
Expand-Archive -Path $destinationZipPath -DestinationPath "C:\Users\YourUsername\Downloads\ExtractedFolder"
